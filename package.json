{
  "name": "astradio",
  "private": true,
  "packageManager": "npm@10.9.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "start": "cd apps/api && npm start",
    "start-web": "cd apps/web && npm run dev",
    "start-api": "cd apps/api && npm run dev"
  },
  "devDependencies": {
    "turbo": "^1.10.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "next": "^14.1.4"
  }
}
